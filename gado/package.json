    "@types/bn.js": "^5.2.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^24.9.1",
    "chai": "^4.3.4",
    "mocha": "^11.6.0",
    "prettier": "^2.6.2",
