  "scripts": {
    "lint:fix": "prettier */*.js \"*/**/*{.js,.ts}\" -w",
    "lint": "prettier */*.js \"*/**/*{.js,.ts}\" --check",
    "keeper:basic": "ts-node keeper-bot.ts",
    "keeper:enhanced": "ts-node -e \"require('./enhanced-keeper-bot').runEnhancedKeeperBot()\"",
    "keeper:build": "tsc --noEmit --skipLibCheck keeper-bot.ts enhanced-keeper-bot.ts inheritance-keeper-bot.ts auto-execution-keeper.ts",
    "keeper:check": "node check-smart-wallets.js",
    "keeper:notification": "ts-node --esm inheritance-keeper-bot.ts",
    "keeper:auto-execution": "ts-node --esm auto-execution-keeper.ts",
    "keeper:start": "./run-keeper.sh",
    "keeper:auto-start": "./run-auto-execution.sh",
    "keeper:compile": "tsc inheritance-keeper-bot.ts --target es2020 --module commonjs --moduleResolution node --esModuleInterop --allowSyntheticDefaultImports --skipLibCheck && tsc auto-execution-keeper.ts --target es2020 --module commonjs --moduleResolution node --esModuleInterop --allowSyntheticDefaultImports --skipLibCheck"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.31.1",
    "@solana/spl-token": "^0.4.14",
